{
  "name": "enharmonics",
  "version": "0.3.0",
  "description": "Get the enharmonics of a given note",
  "main": "enharmonics.js",
  "scripts": {
    "test": "vows test/*.js",
    "docs": "docme README.md",
    "dist": "browserify index.js > dist/index.jsÂ && browserify enharmonics.js | uglifyjs > dist/enharmonics.min.mjs && ls -all dist/*",
    "coverage": "istanbul cover vows test/*.js",
    "cover": "npm run coverage && open ./coverage/lcov-report/index.html",
    "benchmarks": "node test/benchmarks/*.js",
    "climate": "npm run coverage && CODECLIMATE_REPO_TOKEN=the-token codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danigb/enharmonics.git"
  },
  "keywords": [
    "note",
    "pitch",
    "tonal",
    "enharmonics",
    "midi",
    "music",
    "theory"
  ],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/enharmonics/issues"
  },
  "homepage": "https://github.com/danigb/enharmonics#readme",
  "devDependencies": {
    "vows": "^0.8.1"
  },
  "dependencies": {
    "pitch-transpose": "^0.1.1"
  }
}
